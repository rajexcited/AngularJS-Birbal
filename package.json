{
  "name": "angularjs-birbal",
  "version": "0.0.10",
  "description": "chrome extension to analyze Angular app - performance and structure dependency. AddOn features: Dependency graph, http mocks(Allow to mock Http calls for ongoing development).",
  "devDependencies": {
    "grunt": "^0.4.5",
    "time-grunt": "^1.3.0",
    "grunt-contrib-jshint": "^1.0.0",
    "grunt-contrib-concat": "~1.0.0",
    "grunt-contrib-uglify": "~1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-compress": "^1.1.1",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-template": "^0.2.3",
    "grunt-contrib-connect": ">=1.0.1",
    "jit-grunt": "0.10.0",
    "babel-cli": "^6.18.0",
    "babel-preset-react": "^6.16.0"
  },
  "dependencies": {
    "admin-lte": "~2.3.0",
    "font-awesome": "^4.3.0",
    "angular": "^1.4.7",
    "angular-animate": "^1.4.7",
    "angular-mocks": "^1.4.7",
    "jquery": "^2.1.4",
    "floatthead": "^1.4.0",
    "ion-rangeslider": "^2.1.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  },
  "scripts": {
    "clean": "npm prune & npm update & grunt clean",
    "babel": "babel src/popup/*.jsx --out-file src/popup/react-components.generated.js --presets=react",
    "build": "npm run clean & npm run babel & grunt build --dev",
    "package": "npm run clean & npm run babel & grunt build-extension --prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rajexcited/AngularJS-Birbal.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "devtools",
    "debug",
    "analysis",
    "chrome",
    "extension",
    "performance",
    "birbal",
    "birbalJS",
    "graph",
    "dependency",
    "$http",
    "$httpBackend",
    "mock http"
  ],
